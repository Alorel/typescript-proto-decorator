{
  "name":            "typescript-proto-decorator",
  "version":         "1.0.1",
  "description":     "Sets a value on a class' prototype",
  "main":            "dist/es5/ProtoDecorator.js",
  "es2015":          "dist/esm2015/ProtoDecorator.js",
  "module":          "dist/esm5/ProtoDecorator.js",
  "types":           "dist/es5/ProtoDecorator.d.ts",
  "scripts":         {
    "clean:test":    "rimraf ./test/*.js ./test/*.js.map ./.nyc_output ./src/*.js ./src/*.js.map",
    "pretest":       "concurrently \"npm run clean:test\" \"rimraf ./.coverage\" \"npm run build\" && tsc -p tsconfig.test.json",
    "test":          "nyc ava --no-cache --color --verbose",
    "posttest":      "npm run clean:test",
    "reinstall":     "rm -rf node_modules package-lock.json && npm install",
    "prebuild":      "rimraf ./dist",
    "build":         "concurrently \"npm run build:esm5\" \"npm run build:esm2015\" \"npm run build:es5\"",
    "build:esm5":    "tsc -p tsconfig.esm5.json",
    "build:esm2015": "tsc -p tsconfig.esm2015.json",
    "build:es5":     "tsc -p tsconfig.es5.json",
    "watch":         "concurrently \"npm run watch:esm5\" \"npm run watch:esm2015\" \"npm run watch:es5\"",
    "watch:esm5":    "tsc -p tsconfig.esm5.json --watch",
    "watch:esm2015": "tsc -p tsconfig.esm2015.json --watch",
    "watch:es5":     "tsc -p tsconfig.es5.json --watch",
    "lint":          "tslint -p ./tsconfig.base.json -s node_modules/custom-tslint-formatters/formatters -t grouped '**/*.ts'",
    "lint:fix":      "tslint --fix -p ./tsconfig.base.json -s node_modules/custom-tslint-formatters/formatters -t grouped '**/*.ts'",
    "precommit":     "npm run lint:fix",
    "prepush":       "npm run lint"
  },
  "repository":      {
    "type": "git",
    "url":  "git+https://github.com/Alorel/typescript-proto-decorator.git"
  },
  "keywords":        [
    "typescript",
    "decorator",
    "prototype"
  ],
  "author":          {
    "name":  "Arturas Molcanovas",
    "url":   "https://alorel.github.io",
    "email": "a.molcanovas@gmail.com"
  },
  "license":         "MIT",
  "bugs":            {
    "url": "https://github.com/Alorel/typescript-proto-decorator/issues"
  },
  "homepage":        "https://github.com/Alorel/typescript-proto-decorator",
  "devDependencies": {
    "@types/node":              "^8.0.47",
    "ava":                      "^0.23.0",
    "concurrently":             "^3.5.0",
    "coveralls":                "^3.0.0",
    "custom-tslint-formatters": "^2.1.1",
    "greenkeeper-lockfile":     "^1.12.0",
    "husky":                    "^0.14.3",
    "nyc":                      "^11.3",
    "rimraf":                   "^2.6.2",
    "tslint":                   "^5.8.0",
    "typescript":               "^2.6.0"
  },
  "dependencies":    {},
  "snyk":            true
}
