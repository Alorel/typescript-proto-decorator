{
  "name": "typescript-proto-decorator",
  "version": "2.0.1",
  "description": "Sets a value on a class' prototype",
  "main": "dist/es5/ProtoDecorator.js",
  "browser": "dist/umd/ProtoDecorator.js",
  "es2015": "dist/esm2015/ProtoDecorator.js",
  "module": "dist/esm5/ProtoDecorator.js",
  "types": "dist/es5/ProtoDecorator.d.ts",
  "typings": "dist/es5/ProtoDecorator.d.ts",
  "scripts": {
    "test": "cross-env TS_NODE_PROJECT=./tsconfig.test.json node_modules/.bin/mocha --opts ./mocha.opts",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "build": "concurrently \"npm run build:esm5\" \"npm run build:esm2015\" \"npm run build:es5\" \"npm run build:umd\"",
    "prebuild:esm5": "rimraf dist/esm5",
    "build:esm5": "tsc -p tsconfig.esm5.json",
    "prebuild:esm2015": "rimraf dist/esm2015",
    "build:esm2015": "tsc -p tsconfig.esm2015.json",
    "prebuild:es5": "rimraf dist/es5",
    "build:es5": "tsc -p tsconfig.es5.json",
    "prebuild:umd": "rimraf dist/umd",
    "build:umd": "webpack",
    "watch": "concurrently \"npm run watch:esm5\" \"npm run watch:esm2015\" \"npm run watch:es5\"",
    "watch:esm5": "npm run build:esm5 -- --watch",
    "watch:esm2015": "npm run build:esm2015 -- --watch",
    "watch:es5": "npm run build:es5 -- --watch",
    "watch:umd": "webpack --watch",
    "lint": "tslint -p ./tsconfig.base.json -s node_modules/custom-tslint-formatters/formatters -t grouped",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alorel/typescript-proto-decorator.git"
  },
  "keywords": [
    "typescript",
    "decorator",
    "prototype"
  ],
  "author": {
    "name": "Arturas Molcanovas",
    "url": "https://alorel.github.io",
    "email": "a.molcanovas@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alorel/typescript-proto-decorator/issues"
  },
  "homepage": "https://github.com/Alorel/typescript-proto-decorator",
  "devDependencies": {
    "awesome-typescript-loader": "^5.0.0",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^10.0.3",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "custom-tslint-formatters": "^2.1.1",
    "mocha": "^5.0.5",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.4",
    "ts-node": "^6.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.0.0"
  },
  "dependencies": {}
}
