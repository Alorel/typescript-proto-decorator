{
  "name": "proto-decorator",
  "version": "2.1.0",
  "description": "Sets a value on a class' prototype",
  "main": "dist/es5/ProtoDecorator.js",
  "browser": "dist/umd/ProtoDecorator.js",
  "jsdelivr": "dist/umd/ProtoDecorator.min.js",
  "es2015": "dist/esm2015/ProtoDecorator.js",
  "esm2015": "dist/esm2015/ProtoDecorator.js",
  "fesm2015": "dist/esm2015/ProtoDecorator.js",
  "module": "dist/esm5/ProtoDecorator.js",
  "esm5": "dist/esm5/ProtoDecorator.js",
  "fesm5": "dist/esm5/ProtoDecorator.js",
  "types": "dist/es5/ProtoDecorator.d.ts",
  "typings": "dist/es5/ProtoDecorator.d.ts",
  "scripts": {
    "pretest:legacy": "babel --config-file ./.babelrc.legacy -o test/Proto.legacy.js test/ProtoDecorator.js",
    "pretest:new": "babel --config-file ./.babelrc.new -o test/Proto.new.js test/ProtoDecorator.js",
    "test": "npm --scripts-prepend-node-path=auto run test:legacy && npm --scripts-prepend-node-path=auto run test:new",
    "test:legacy": "mocha --opts ./mocha.opts test/Proto.legacy.js",
    "test:new": "mocha --opts ./mocha.opts test/Proto.new.js",
    "pretest": "npm --scripts-prepend-node-path=auto run build:es5",
    "build": "concurrently \"npm:build:*\"",
    "prebuild:esm5": "rimraf dist/esm5",
    "build:esm5": "tsc -p tsconfig.esm5.json",
    "prebuild:esm2015": "rimraf dist/esm2015",
    "build:esm2015": "tsc -p tsconfig.esm2015.json",
    "prebuild:es5": "rimraf dist/es5",
    "build:es5": "tsc -p tsconfig.es5.json",
    "prebuild:umd": "rimraf dist/umd",
    "build:umd": "webpack",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:esm5": "npm --scripts-prepend-node-path=auto run build:esm5 -- --watch",
    "watch:esm2015": "npm --scripts-prepend-node-path=auto run build:esm2015 -- --watch",
    "watch:es5": "npm --scripts-prepend-node-path=auto run build:es5 -- --watch",
    "watch:umd": "npm --scripts-prepend-node-path=auto run build:umd -- --watch",
    "lint": "tslint -p ./tsconfig.base.json -s node_modules/custom-tslint-formatters/formatters -t grouped",
    "lint:fix": "npm --scripts-prepend-node-path=auto run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alorel/typescript-proto-decorator.git"
  },
  "keywords": [
    "typescript",
    "decorator",
    "prototype"
  ],
  "author": {
    "name": "Arturas Molcanovas",
    "url": "https://alorel.github.io",
    "email": "a.molcanovas@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alorel/typescript-proto-decorator/issues"
  },
  "homepage": "https://github.com/Alorel/typescript-proto-decorator",
  "devDependencies": {
    "@alorel-personal/conventional-changelog-alorel": "^2.0.3",
    "@alorel-personal/semantic-release": "^1.3.0",
    "@alorel-personal/tslint-rules": "^4.0.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.1",
    "@semantic-release/npm": "^5.0.2",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^11.9.0",
    "chai": "^4.1.2",
    "concurrently": "^4.0.0",
    "custom-tslint-formatters": "^2.1.1",
    "lodash": "^4.17.10",
    "mocha": "^5.0.5",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.9",
    "source-map-support": "^0.5.4",
    "ts-loader": "^5.3.0",
    "tslint": "^5.8.0",
    "typescript": "^3.0.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.0.0"
  },
  "dependencies": {}
}
