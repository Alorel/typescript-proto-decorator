{
  "name": "proto-decorator",
  "version": "3.0.0",
  "description": "Sets a value on a class' prototype",
  "main": "ProtoDecorator.js",
  "browser": "_bundle/umd.js",
  "jsdelivr": "_bundle/umd.min.js",
  "esm5": "_bundle/esm5/ProtoDecorator.js",
  "esm2015": "_bundle/esm2015/ProtoDecorator.js",
  "types": "ProtoDecorator.d.ts",
  "module": "_bundle/esm5/ProtoDecorator.js",
  "typings": "ProtoDecorator.d.ts",
  "scripts": {
    "pretest:legacy": "babel --config-file ./.babelrc.legacy -o test/Proto.legacy.js test/ProtoDecorator.js",
    "pretest:new": "babel --config-file ./.babelrc.new -o test/Proto.new.js test/ProtoDecorator.js",
    "test": "concurrently \"npm:test:*\"",
    "test:legacy": "mocha --opts ./mocha.opts test/Proto.legacy.js",
    "test:new": "mocha --opts ./mocha.opts test/Proto.new.js",
    "pretest": "alo build --targets cjs",
    "build": "alo build",
    "prewatch": "npm --scripts-prepend-node-path=auto run prebuild",
    "watch": "tsc -p tsconfig.es5.json --watch",
    "lint": "alo tslint -p ./tsconfig.base.json",
    "lint:fix": "npm --scripts-prepend-node-path=auto run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alorel/typescript-proto-decorator.git"
  },
  "keywords": [
    "typescript",
    "babel",
    "decorator",
    "prototype",
    "proto"
  ],
  "author": {
    "name": "Arturas Molcanovas",
    "url": "https://alorel.github.io",
    "email": "a.molcanovas@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alorel/typescript-proto-decorator/issues"
  },
  "homepage": "https://github.com/Alorel/typescript-proto-decorator",
  "devDependencies": {
    "@alorel-personal/build-tools": "^4.4.6",
    "@alorel-personal/conventional-changelog-alorel": "^2.0.3",
    "@alorel-personal/semantic-release": "^1.3.0",
    "@alorel-personal/tslint-rules": "^4.0.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/exec": "^3.3.2",
    "@semantic-release/git": "^7.0.1",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.0.2",
    "@types/node": "^11.9.0",
    "chai": "^4.1.2",
    "concurrently": "^4.0.0",
    "mocha": "^6.0.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.9",
    "typescript": "^3.0.0"
  },
  "dependencies": {}
}
