{
  "name": "proto-decorator",
  "version": "3.0.2",
  "description": "Sets a value on a class' prototype",
  "main": "ProtoDecorator.js",
  "scripts": {
    "test:babel:legacy": "babel --config-file ./.babelrc.legacy -o test/Proto.legacy.js test/ProtoDecorator.js",
    "test:babel:new": "babel --config-file ./.babelrc.new -o test/Proto.new.js test/ProtoDecorator.js",
    "test:babel": "concurrently \"npm:test:babel:*\"",
    "test": "nyc mocha --opts ./mocha.opts test/Proto.legacy.js test/Proto.new.js",
    "pretest": "concurrently -n build,clean,babel,preprocess \"alo build --targets cjs\" \"rimraf coverage\" \"npm run test:babel && ts-node pretest-postprocess.ts\"",
    "build": "alo build",
    "prewatch": "npmrun prebuild",
    "watch": "tsc -p tsconfig.es5.json --watch",
    "lint": "alo tslint -p ./tsconfig.base.json",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alorel/typescript-proto-decorator.git"
  },
  "keywords": [
    "typescript",
    "babel",
    "decorator",
    "prototype",
    "proto"
  ],
  "author": {
    "name": "Arturas Molcanovas",
    "url": "https://alorel.github.io",
    "email": "a.molcanovas@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alorel/typescript-proto-decorator/issues"
  },
  "homepage": "https://github.com/Alorel/typescript-proto-decorator",
  "devDependencies": {
    "@alorel-personal/build-tools": "4.4.7",
    "@alorel-personal/conventional-changelog-alorel": "2.0.4",
    "@alorel-personal/semantic-release": "1.4.0",
    "@alorel-personal/tslint-rules": "4.1.1",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/exec": "^3.3.2",
    "@semantic-release/git": "^7.0.1",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.0.2",
    "@types/node": "^11.9.0",
    "chai": "^4.1.2",
    "concurrently": "^4.0.0",
    "coveralls": "^3.0.3",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.9",
    "source-map-support": "^0.5.10",
    "ts-node": "^8.0.2",
    "typescript": "^3.0.0"
  },
  "dependencies": {}
}